

let a=prompt("introduzca pimer valor");
let b=prompt("introduzca segundo valor");
let c=prompt("introduzca tercer valor");


    if(a==b && a==c){
        alert("triangulo equilatero");
    }

    else if(a!=b && a!=c && b!=c){
        alert("triangulo escaleno");
    }

    else if(a==b || a==c || b==c){
      alert("triangulo isoceles");
    }
